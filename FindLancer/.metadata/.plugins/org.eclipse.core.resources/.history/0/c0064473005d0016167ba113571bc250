package br.com.findlancer.tests;



public class Testes {
	
 public static void main(String[] args) {
	 
	 String telefone="(33)3333-3333";
	    System.out.println(telefone);
	    telefone=telefone.replace("(", "");
	    System.out.println(telefone);
		telefone=telefone.replace(")", "");
		System.out.println(telefone);
		telefone=telefone.replace("-", "");
		System.out.println(telefone);
}
}
