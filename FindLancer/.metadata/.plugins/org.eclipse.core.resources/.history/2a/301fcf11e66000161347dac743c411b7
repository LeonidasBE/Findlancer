public class Main {
	public static void main(String[] args) {

		//laco for
		for(int i=1;i<=10;i++){
			for(int i2=1;i2<=10;i2++){
				System.out.println("Tabuada de "+i+" : "+i+" x "+i2+" = "+(i*i2) );
			}
			System.out.println(" ");
		}
		
		int i=1; 
		int i2=1; 
		while(i<10){
			i++;
			while(i2<10){
				i2++;
				System.out.println("Tabuada de "+i+" : "+i+" x "+i2+" = "+(i*i2));
			}
			System.out.println(" ");
			i2=0;
		}
	}
}
