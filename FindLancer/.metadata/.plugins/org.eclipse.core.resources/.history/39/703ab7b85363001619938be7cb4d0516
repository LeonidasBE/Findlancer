package br.com.findlancer.tests;

import java.util.ArrayList;
import java.util.List;

import br.com.findlancer.model.Cidade;
import br.com.findlancer.model.Endereco;
import br.com.findlancer.model.Freelancer;
import br.com.findlancer.sql.LoginDAO;

public class Testes {
	
 public static void main(String[] args) {
	  
	 ArrayList<List> listageral = new ArrayList<List>();
	 
	 List<Freelancer> listafree = new ArrayList<Freelancer>();
	 List<Endereco> listaend = new ArrayList<Endereco>();
	 List<Cidade> listacid = new ArrayList<Cidade>();
	 
	 Freelancer free = new Freelancer();
	 free.setNome("marcos");
	 Endereco end = new Endereco();
	 end.setBairro("velha");
	 Cidade cid = new Cidade();
	 cid.setNomeCidade("bnu");
	 
	 listafree.add(free);
	 listaend.add(end);
	 listacid.add(cid);
	 
	 listageral.add(listaend);	 
	 listageral.add(listafree);
	 listageral.add(listacid);
	 
	  
	 
	 List<Freelancer> listfree = new ArrayList<Freelancer>();
	 
	 listfree=listageral.get(1);
	 
	 for(Freelancer freezao : listfree){
		 System.out.println(freezao.getNome());
	 }
 }
}
