public class Ex11 {
	public static void main(String[] args) {
		 
		char opcao = 'S';
		
		switch(opcao){
			case 'S':
				System.out.println("Solteiro");
				break;
			case 'C':
				System.out.println("Casado");
				break;
			case 'V':
				System.out.println("Viuvo");
				break;
			case 'D':
				System.out.println("Divorciado");
				break;
			default:
				System.out.println("Opcao invalida");
			
		}
		
	}
	

