public class Main {
	public static void main(String[] args) {
		int numero=50;
		switch(numero){
		case 50:
			System.out.println("Reprovado");
			break;
		case 75:
			System.out.println("A recuperar");
			break;
		case 90:
			System.out.println("Aprovado");
			break;
		case 100:
			System.out.println("Aprovado com louvor");
			break;
			
			default:
				System.out.println("Pontuação invalida");
		}
	}
}