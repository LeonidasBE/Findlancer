package br.com.findlancer.entidades;

import java.io.IOException;
import javax.servlet.ServletException;
import javax.servlet.annotation.WebServlet;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

import br.com.findlancer.sql.FreelancerDAO;

/**
 * Servlet implementation class FreelancerController
 */
@WebServlet("/freelacontroller.do")
public class FreelancerController extends HttpServlet {
	private static final long serialVersionUID = 1L;
       
     
    public FreelancerController() {
        super();
         
    }
 
	protected void doGet(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {

		    String nascimento;
		    String categoria;
			String subcategoria;
			String formacao;
			String experiencia;
			String nome;
			String telefone;
			String email;
			String senha;
			String descricao;
		    String cidade;
		
		 
		 
		 Freelancers usuario1 = new Freelancers();
		 usuario1.setNome(nome);
		 usuario1.setSenha(senha);
		 
		 FreelancerDAO usuarioSQL = new FreelancerDAO();
		 usuarioSQL.Cadastrar(usuario1); 
		
		
	}
 
	protected void doPost(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
		 
	}

}
